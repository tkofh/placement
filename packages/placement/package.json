{
  "name": "placement",
  "version": "0.1.2",
  "description": "Layout algorithms",
  "author": "Tim Morris <timthewebguy@gmail.com>",
  "homepage": "https://github.com/tkofh/placement#readme",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/Frame.cjs",
  "module": "./dist/Frame.mjs",
  "types": "./dist/Frame.d.ts",
  "exports": {
    ".": {
      "require": "./dist/Frame.cjs",
      "import": "./dist/Frame.mjs",
      "types": "./dist/Frame.d.ts"
    },
    "./Box": {
      "require": "./dist/Box.cjs",
      "import": "./dist/Box.mjs",
      "types": "./dist/Box.d.ts"
    },
    "./Frame": {
      "require": "./dist/Frame.cjs",
      "import": "./dist/Frame.mjs",
      "types": "./dist/Frame.d.ts"
    },
    "./FlexFrame": {
      "require": "./dist/FlexFrame.cjs",
      "import": "./dist/FlexFrame.mjs",
      "types": "./dist/FlexFrame.d.ts"
    },
    "./properties/NumericProperty": {
      "require": "./dist/properties/NumericProperty.cjs",
      "import": "./dist/properties/NumericProperty.mjs",
      "types": "./dist/properties/NumericProperty.d.ts"
    },
    "./properties/RatioProperty": {
      "require": "./dist/properties/RatioProperty.cjs",
      "import": "./dist/properties/RatioProperty.mjs",
      "types": "./dist/properties/RatioProperty.d.ts"
    },
    "./properties/QuantityProperty": {
      "require": "./dist/properties/QuantityProperty.cjs",
      "import": "./dist/properties/QuantityProperty.mjs",
      "types": "./dist/properties/QuantityProperty.d.ts"
    },
    "./utils": {
      "require": "./dist/utils.cjs",
      "import": "./dist/utils.mjs",
      "types": "./dist/utils.d.ts"
    }
  },
  "files": ["dist"],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkofh/placement.git"
  },
  "scripts": {
    "build": "unbuild",
    "test": "vitest",
    "test:run": "vitest run",
    "prepublish": "bun run build"
  },
  "bugs": {
    "url": "https://github.com/tkofh/placement/issues"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.5.0",
    "@vitest/ui": "^1.5.0",
    "unbuild": "^2.0.0",
    "vitest": "^1.5.0"
  },
  "dependencies": {
    "gpds": "^0.0.12"
  }
}
