{
  "name": "placement",
  "version": "0.2.0",
  "description": "Layout algorithms",
  "author": "Tim Morris <timthewebguy@gmail.com>",
  "homepage": "https://github.com/tkofh/placement#readme",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./flexbox": {
      "require": "./dist/flexbox.cjs",
      "import": "./dist/flexbox.mjs",
      "types": "./dist/flexbox.d.ts"
    },
    "./frame": {
      "require": "./dist/frame.cjs",
      "import": "./dist/frame.mjs",
      "types": "./dist/frame.d.ts"
    },
    "./interval": {
      "require": "./dist/interval.cjs",
      "import": "./dist/interval.mjs",
      "types": "./dist/interval.d.ts"
    },
    "./point": {
      "require": "./dist/point.cjs",
      "import": "./dist/point.mjs",
      "types": "./dist/point.d.ts"
    },
    "./rect": {
      "require": "./dist/rect.cjs",
      "import": "./dist/rect.mjs",
      "types": "./dist/rect.d.ts"
    },
    "./track": {
      "require": "./dist/track.cjs",
      "import": "./dist/track.mjs",
      "types": "./dist/track.d.ts"
    },
    "./utils": {
      "require": "./dist/utils.cjs",
      "import": "./dist/utils.mjs",
      "types": "./dist/utils.d.ts"
    }
  },
  "files": ["dist"],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkofh/placement.git"
  },
  "scripts": {
    "run": "tsx ./src/flexbox.ts",
    "build": "unbuild",
    "test": "vitest",
    "test:run": "vitest run",
    "prepublish": "bun run build"
  },
  "bugs": {
    "url": "https://github.com/tkofh/placement/issues"
  },
  "devDependencies": {
    "@types/node": "^20.14.7",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "tsx": "^4.15.7",
    "unbuild": "^2.0.0",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "valued": "^0.2.0"
  }
}
