{
  "name": "placement",
  "version": "0.1.0",
  "description": "Layout algorithms",
  "author": "Tim Morris <timthewebguy@gmail.com>",
  "homepage": "https://github.com/tkofh/placement#readme",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./flexbox": {
      "require": "./dist/flexbox.cjs",
      "import": "./dist/flexbox.mjs",
      "types": "./dist/flexbox.d.ts"
    },
    "./primitives": {
      "require": "./dist/primitives.cjs",
      "import": "./dist/primitives.mjs",
      "types": "./dist/primitives.d.ts"
    },
    "./utils": {
      "require": "./dist/utils.cjs",
      "import": "./dist/utils.mjs",
      "types": "./dist/utils.d.ts"
    }
  },
  "files": ["dist"],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkofh/placement.git"
  },
  "scripts": {
    "build": "unbuild",
    "test": "vitest",
    "test:run": "vitest run",
    "prepublish": "bun run build"
  },
  "bugs": {
    "url": "https://github.com/tkofh/placement/issues"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "unbuild": "^2.0.0",
    "vitest": "^1.3.1"
  }
}
