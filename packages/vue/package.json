{
  "name": "@placement/vue",
  "version": "0.0.0",
  "description": "Vue components for Placement",
  "author": "Tim Morris <timthewebguy@gmail.com>",
  "homepage": "https://github.com/tkofh/placement#readme",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./AbsoluteFrame": {
      "require": "./dist/components/AbsoluteFrame.cjs",
      "import": "./dist/components/AbsoluteFrame.mjs",
      "types": "./dist/components/AbsoluteFrame.d.ts"
    },
    "./BasicFrame": {
      "require": "./dist/components/BasicFrame.cjs",
      "import": "./dist/components/BasicFrame.mjs",
      "types": "./dist/components/BasicFrame.d.ts"
    },
    "./FlexFrame": {
      "require": "./dist/components/FlexFrame.cjs",
      "import": "./dist/components/FlexFrame.mjs",
      "types": "./dist/components/FlexFrame.d.ts"
    },
    "./RootFrame": {
      "require": "./dist/components/RootFrame.cjs",
      "import": "./dist/components/RootFrame.mjs",
      "types": "./dist/components/RootFrame.d.ts"
    },
    "./useFrame": {
      "require": "./dist/composables/useFrame.cjs",
      "import": "./dist/composables/useFrame.mjs",
      "types": "./dist/composables/useFrame.d.ts"
    },
    "./useRootRect": {
      "require": "./dist/composables/useRootRect.cjs",
      "import": "./dist/composables/useRootRect.mjs",
      "types": "./dist/composables/useRootRect.d.ts"
    },
    "./nuxt": {
      "require": "./dist/nuxt.cjs",
      "import": "./dist/nuxt.mjs",
      "types": "./dist/nuxt.d.ts"
    }
  },
  "files": ["dist"],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkofh/placement.git"
  },
  "scripts": {
    "build": "unbuild"
  },
  "dependencies": {
    "@nuxt/kit": "^3.11.2",
    "@nuxt/schema": "^3.11.2",
    "placement": "workspace:^",
    "vue": "^3.4.24"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0"
  }
}
